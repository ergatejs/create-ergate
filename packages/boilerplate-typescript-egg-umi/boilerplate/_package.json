{
  "name": "{{name}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "autod": "autod",
    "start": "egg-scripts start --daemon --title={{name}}",
    "stop": "egg-scripts stop --title={{name}}",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "build": "APP_ROOT=$PWD/app/view umi build",
    "cov": "egg-bin cov",
    "clean": "ets clean",
    "ci": "npm run lint && npm run cov",
    "tsc": "ets && tsc -p tsconfig.server.json",
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test",
    "lint": "npm run be-lint && npm run fe-lint",
    "be-lint": "eslint --fix --ext .ts --ignore-pattern 'app/view/' .",
    "fe-lint": "eslint --fix --ext .ts --ext .tsx -c app/view/.eslintrc app/view/",
    "fe-prettier": "prettier --write --config app/view/.prettierrc 'app/view/**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "gitHooks": {
    "pre-commit": ""
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "egg": "^2.6.1",
    "egg-jwt": "^3.1.7",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^5.2.1",
    "egg-view-assets": "^1.6.0",
    "egg-view-nunjucks": "^2.2.0",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.1.0",
    "@ant-design/pro-layout": "^5.0.9",
    "@types/mocha": "^2.2.40",
    "@types/node": "^14.11.1",
    "@types/supertest": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "@umijs/preset-react": "^1.4.8",
    "antd": "^4.1.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.15.0",
    "egg-mock": "^3.16.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-egg": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "lint-staged": "^10.1.2",
    "prettier": "^2.0.4",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0",
    "umi": "^3.0.16"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "type": "github",
    "os": {
      "github": "linux"
    },
    "version": "12"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "{{author}}",
  "license": "MIT"
}
